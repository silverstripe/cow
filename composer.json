{
  "name": "silverstripe/cow",
  "description": "Release Tool for SilverStripe",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Damian Mooyman",
      "email": "damian@silverstripe.com"
    }
  ],
  "bin": [
    "bin/cow"
  ],
  "autoload": {
    "psr-4": {
      "SilverStripe\\Cow\\Tests\\": "tests/",
      "SilverStripe\\Cow\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5",
    "symfony/console": "^3.2",
    "symfony/process": "^3.2",
    "symfony/yaml": "^3.2",
    "gitonomy/gitlib": "~1.0",
    "psr/log": "^1",
    "knplabs/github-api": "^2.1",
    "php-http/guzzle6-adapter": "^1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
